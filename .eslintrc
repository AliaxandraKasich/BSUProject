{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-use-before-define": "off",
    "no-var": "off",
    "no-param-reassign": "off",
    "vars-on-top": "off",
    "no-plusplus": "off",
    "prefer-template": "off", //concat
    "no-console": "off",
    "func-names": "off",
    "no-underscore-dangle": "off"
  }
}